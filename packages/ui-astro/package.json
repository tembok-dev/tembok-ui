{
  "name": "@tembok/ui-astro",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tembok-dev/tembok-ui.git",
    "directory": "packages/ui-astro"
  },
  "homepage": "https://github.com/tembok-dev/tembok-ui#readme",
  "bugs": {
    "url": "https://github.com/tembok-dev/tembok-ui/issues"
  },
  "type": "module",
  "main": "./src/index.ts",
