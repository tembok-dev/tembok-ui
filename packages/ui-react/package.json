{
  "name": "@tembok/ui-react",
  "version": "0.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tembok-dev/tembok-ui.git",
    "directory": "packages/ui-react"
  },
  "homepage": "https://github.com/tembok-dev/tembok-ui#readme",
  "bugs": {
    "url": "https://github.com/tembok-dev/tembok-ui/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
